# Docker Compose Environment Variables

# Database
DB_USER=orchestrator
DB_PASSWORD=orchestrator_password_change_me
DB_NAME=orchestrator
DB_PORT=5432

# Redis
REDIS_PORT=6379

# OpenAI (for agent runner)
OPENAI_API_KEY=your_openai_api_key_here

# Logging
LOG_LEVEL=info

# Worker replicas (for scaling)
HTTP_WORKER_REPLICAS=2
AGENT_WORKER_REPLICAS=2

# Optional: External URLs (if not using internal DNS)
# ORCHESTRATOR_URL=http://orchestrator:8081
# REDIS_URL=redis://redis:6379
